<header id="header">
  <div id="banner"></div>
  <div id="header-outer" class="outer">
    <div id="header-title" class="inner">
      <h1 id="logo-wrap">
        <a href="<%- url_for() %>" id="logo"><%= config.title %></a>
      </h1>
      <% if (theme.subtitle){ %>
        <h2 id="subtitle-wrap">
          <a href="<%- url_for() %>" id="subtitle"><%= theme.subtitle %></a>
        </h2>
      <% } %>
    </div>
    <div id="header-inner" class="inner">
      <nav id="main-nav">
        <a id="main-nav-toggle" class="nav-icon"></a>
        <% site.pages.forEach(function(page, i){ %>
          <% if(!page.home) { %>
           <a href="<%=url_for(page.path)%>" title="<%=page.title%>"><%=page.title%></a>
          <%}%>
        <% }) %>
      </nav>
    </div>
  </div>
</header>